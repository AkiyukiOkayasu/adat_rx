/// ADAT共通パッケージ
///
/// ADAT RX/TX間で共有する型定義と定数を提供する。
pub package adat_pkg {
    /// ADATビットレートファミリー
    ///
    /// 物理ビットレートは2種類のみ:
    /// - F44K1: 44.1kHz × 256 = 11,289,600 bps (44.1kHz / 88.2kHz / 176.4kHz 系)
    /// - F48K:  48kHz × 256 = 12,288,000 bps (48kHz / 96kHz / 192kHz 系)
    enum AdatFamily {
        /// 44.1kHz系 (11,289,600 bps)
        F44K1,
        /// 48kHz系 (12,288,000 bps)
        F48K,
    }
}
